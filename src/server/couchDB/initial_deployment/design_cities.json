{
  "_id": "_design/cities",
  "views": {
    "get_timestamp": {
      "reduce": "_count",
      "map": "function (doc) {\n  if(doc.place === null){\n    emit([doc.db_timestamp,\"Missing\"], 1);\n  }else{\n    emit([doc.db_timestamp,doc.place.name], 1);\n  }\n}"
    },
    "by_ts_city": {
      "reduce": "_count",
      "map": "function (doc) {\n  if(doc.place === null){\n    emit([doc.db_timestamp,\"Missing\"], 1);\n  }else{\n    emit([doc.db_timestamp,doc.place.name], 1);\n  }\n}"
    },
    "by_city": {
      "reduce": "_count",
      "map": "function (doc) {\n  if(doc.place.place_type == \"city\"){\n    emit(doc.place.name, 1);\n  }\n}"
    }
  },
  "language": "javascript"
}